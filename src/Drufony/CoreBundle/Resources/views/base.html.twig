    <div class="layout-app">

    <h5 class="innerAll margin-none">

      {% if breadCrumb is defined %}
      {% for crumb in breadCrumb %}
        {% if loop.index != breadCrumb|length %}
        <a href="{{ path(crumb.url, { 'lang': lang }) }}">{{ crumb.label }}</a> »
        {% else %}
          {{crumb.label }}
        {% endif %}
      {% endfor %}
      {% endif %}

    </h5>

      {% for type, flashMessages in app.session.flashbag.all() %}
          {% set flashClass = '' %}
          {% if type == constant('INFO') %}
            {% set flashClass = "alert alert-success" %}
          {% endif %}
          {% if type == constant('ERROR') %}
            {% set flashClass = "alert alert-danger" %}
          {% endif %}
          {% for message in flashMessages %}
            <div class="col-md-12">
              <div class="flash-{{type}} {{flashClass}}" style="opacity: .8">
              <button type="button" class="close" data-dismiss="alert">×</button>
                  {{ message }}
              </div>
            </div>
          {% endfor %}
      {% endfor %}


